{
  "name": "discraft",
  "version": "1.6.9-beta.2",
  "description": "Create and deploy production Discord bots in minutes",
  "type": "module",
  "packageManager": "bun@1.2.2",
  "scripts": {
    "prepublish": "bun run build",
    "build": "cd package && bun build.ts && bun run sentry:sourcemaps",
    "lint": "eslint . --no-cache",
    "typecheck": "tsc --noEmit",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org bestcodes-official --project discraft-npm ./package/dist && sentry-cli sourcemaps upload --org bestcodes-official --project discraft-npm ./package/dist"
  },
  "bin": {
    "discraft": "package/dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/The-Best-Codes/discraft-js.git"
  },
  "keywords": [
    "discord",
    "bot",
    "framework",
    "cli"
  ],
  "files": [
    "package/dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "author": "The-Best-Codes",
  "contributors": [
    {
      "name": "BestCodes",
      "email": "admin@bestcodes.dev",
      "url": "https://bestcodes.dev"
    },
    {
      "name": "Arnav K",
      "url": "https://github.com/ArnavK-09"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/The-Best-Codes/discraft-js/issues"
  },
  "homepage": "https://github.com/The-Best-Codes/discraft-js#readme",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/bun": "^1.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.1",
    "bun": "^1.2.2",
    "eslint": "^9.20.0",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.23.0"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@inquirer/core": "^10.1.6",
    "@sentry/cli": "^2.41.1",
    "@sentry/node": "^8.54.0",
    "@sentry/profiling-node": "^8.54.0",
    "chokidar": "^4.0.3",
    "commander": "^13.1.0",
    "consola": "^3.4.0",
    "esbuild": "^0.25.0",
    "esbuild-node-externals": "^1.16.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "inquirer": "^12.4.1",
    "kleur": "^4.1.5"
  },
  "trustedDependencies": [
    "@sentry/cli"
  ]
}